{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**", "dist/**"]
    },
    "lint": {},
    "test": {},
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}